{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "allowJs": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "target": "ES2022",
    "moduleResolution": "Node"
  }
}
